<script>


    $(document).ready(function () {
        $('#addimgBtn').click(function () {
            var formData = new FormData($('#trending_add_img')[0]);

            $.ajax({
                type: 'POST',
                url: '/trending_add_img',
                data: formData,
                processData: false,
                contentType: false,
                success: function(res){
                    Swal.fire({
                        icon: "success",
                        title: "Success",
                        text: " Added to trending image successfully"
                    }).then(()=>{
                        location.reload()
                    })
                },
                error: function(err){
                    Swal.fire({
                        icon: "error",
                        title: "Error",
                        text: "Failed to add product to car"
                    });
                }
            });
        });
    });
</script>

